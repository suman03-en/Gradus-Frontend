{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://YOUR_BACKEND_URL.com/api/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://YOUR_BACKEND_URL.com/admin/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://YOUR_BACKEND_URL.com/media/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
